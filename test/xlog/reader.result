trun = require('test_run').new()
---
...
pattern_prefix = fio.pathjoin(os.getenv("SOURCEDIR"), "test/xlog/reader")
---
...
pattern_prefix_re = pattern_prefix:gsub("/", "\\/")
---
...
trun:cmd(("push filter '%s' to '%s'"):format(pattern_prefix_re, "<prefix>"))
