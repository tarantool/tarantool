      - run_max: 1
      - size: 248
      - size_uncompressed: 248
      - temperature_avg: 0
      - temperature_max: 0
      - temperature_min: 0
  - memory:
    - limit: 53687091
    - used: 58
  - metric:
    - lsn: 5
    - nsn: 1
  - performance:
    - cursor: 1
    - cursor_latency: <cursor_latency>
    - cursor_ops: 0 2 2.0
    - delete: 0
    - delete_latency: 0 0 0.0
    - get: 2
    - get_latency: <get_latency>
    - get_read_cache: 0 0 0.0
    - get_read_disk: 0 0 0.0
    - set: 0
    - set_latency: 0 0 0.0
    - tx: 3
    - tx_active_ro: 0
    - tx_active_rw: 0
    - tx_conflict: 0
    - tx_latency: <tx_latency>
    - tx_ops: 0 1 1.0
    - tx_rollback: 1
    - upsert: 0
    - upsert_latency: 0 0 0.0
    - write_count: 2
  - scheduler:
    - gc_active: <gc_active>
    - zone: '0'
  - vinyl:
    - build: <build>
    - path: <path>
    - version: 2.1.1
    - version_storage: 2.1.1
...
