      - dump_age_wm: 0
      - dump_prio: 0
      - dump_wm: 0
      - gc_period: 0
      - gc_prio: 0
      - gc_wm: 0
  - db:
    - 512/0:
      - count: 2
      - count_dup: 0
      - memory_used: 58
      - node_count: 1
      - page_count: 1
      - read_cache: 0
      - read_disk: 0
      - run_avg: 1
