function _select(_begin, _end)
    a = {}
    while box.info.lsn < _end + 3 do
        box.fiber.sleep(0.001)
    end
    for i = _begin, _end do
        table.insert(a, box.select(0, 0, i))
    end
    return unpack(a)
end;
